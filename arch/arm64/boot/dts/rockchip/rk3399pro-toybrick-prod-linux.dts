/dts-v1/;
#include "rk3399pro-toybrick-prod.dtsi"
#include "rk3399-android.dtsi"
#include "rk3399pro-toybrick-extboard.dtsi"
#include "rk3399pro-toybrick-isp1-ov13850.dtsi"

/ {
	compatible = "rockchip,rk3399pro-toybrick-prod-linux","rockchip,rk3399pro";
};

&extbrd {
	status = "okay";
	io-channels = <&saradc 0>, <&saradc 1>;
	ext-gpios = <&gpio0 RK_PA5 GPIO_ACTIVE_HIGH>,    /* GPIO0_A5 */
		   <&gpio1 RK_PB1 GPIO_ACTIVE_HIGH>,    /* GPIO1_B1 */
		   <&gpio1 RK_PB2 GPIO_ACTIVE_HIGH>,    /* GPIO1_B2 */
		   <&gpio0 RK_PA6 GPIO_ACTIVE_HIGH>;    /* GPIO0_A6 */
};

&route_hdmi {
        status = "disabled";
};

&route_dsi {
        status = "okay";
};

&route_edp {
        status = "okay";
};

&dp_sound {
        status = "disabled";
};

&hdmi_dp_sound {
        status = "disabled";
};

&hdmi_sound {
        status = "okay";
};

&fiq_debugger {
        pinctrl-0 = <&uart2c_xfer>;
};

&rk_key {
        status = "okay";
        io-channels = <&saradc 2>;
        /delete-node/ power-key;
};
