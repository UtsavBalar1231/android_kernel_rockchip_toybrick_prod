/dts-v1/;
#include "rk3399pro-amolk.dtsi"
#include "rk3399-linux.dtsi"

/ {
	compatible = "rockchip,rk3399pro-toybrick-prod-linux", "rockchip,rk3399pro";
	model = "Rockchip RK3399Pro Board";
};

&i2c3 {
	status = "okay";
};

&hdmi {
	status = "okay";
	#sound-dai-cells = <0>;
	ddc-i2c-bus = <&i2c3>;
	rockchip,phy-table =
		<74250000  0x8009 0x0004 0x0272>,
		<165000000 0x802b 0x0004 0x0209>,
		<297000000 0x8039 0x0005 0x028d>,
		<594000000 0x8039 0x0000 0x00f6>,
		<000000000 0x0000 0x0000 0x0000>;
};

&hdmi_in_vopb {
	status = "disabled";
};

&hdmi_in_vopl {
	status = "okay";
};

&route_hdmi {
	status = "okay";
	connect = <&vopl_out_hdmi>;
};

&dsi_in_vopb {
	status = "okay";
};

&dsi_in_vopl {
	status = "disabled";
};

&route_dsi {
	status = "okay";
	connect = <&vopb_out_dsi>;
};

&route_edp {
	status = "disabled";
};

&iep {
	status = "okay";
};

&iep_mmu {
	status = "okay";
};
